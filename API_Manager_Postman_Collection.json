	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Auto login if JWT token is missing or expired",
					"if (!pm.environment.get('JWT_TOKEN')) {",
					"    console.log('JWT Token not found, attempting auto login...');",
					"    ",
					"    pm.sendRequest({",
					"        url: pm.environment.get('BASE_URL') + '/api/auth/login',",
					"        method: 'POST',",
					"        header: {",
					"            'Content-Type': 'application/json'",
					"        },",
					"        body: {",
					"            mode: 'raw',",
					"            raw: JSON.stringify({",
					"                email: 'admin@apimanager.com',",
					"                password: 'admin123'",
					"            })",
					"        }",
					"    }, function (err, response) {",
					"        if (response.code === 200) {",
					"            const data = response.json();",
					"            pm.environment.set('JWT_TOKEN', data.token);",
					"            console.log('Auto login successful, JWT token saved');",
					"        }",
					"    });",
					"}"
				]
			}
		}
